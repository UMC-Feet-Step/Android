        android:layout_marginRight="16dp"
        android:layout_marginTop="13dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/gallery_title_txt"
        app:srcCompat="@drawable/ic_feed_btn_friends" />
